{
    "compilerOptions":
    {
        "target": "es5",
        "module": "es2015",
        "sourceMap": false,
        "declaration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "outDir": "dist",
        "lib": 
        [
            "es6", "dom"
        ],
        "types": 
        [
            "node",
            "crypto-js",
            "jquery-param"
        ],
        "typeRoots": 
        [
            "node_modules/@anglr/types"
        ]
    },
    "exclude": 
    [
        "node_modules",
        "dist",
        "testing",
        "*.spec.ts",
        "karma-test-shim.ts"
    ],
    "angularCompilerOptions": 
    {
        "skipMetadataEmit" : false,
        "genDir": "dist"
    }
}
{
    "name": "@anglr/rest",
    "version": "3.0.3",
    "description": "Angular module representing rest services",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts":
    {
        "build": "npm run ngc",
        "clean": "rimraf dist",
        "clean:deps": "rimraf node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "prepublish": "npm run bundle:package",
        "postpublish": "npm run clean",
        "republish": "npm publish --force",
        "bundle": "webpack",
        "bundle:min": "webpack --env.minify",
        "bundle:package": "npm run clean && npm run build && npm run bundle && npm run bundle:min",
        "deploy:stable": "npp -b",
        "deploy:next": "npp -p -b -s \"beta\" -t \"next\""
    },
    "author": "kukjevov",
    "license": "MIT",
    "dependencies":
    {
        "@angular/core": "^4.0.0",
        "@angular/common": "^4.0.0",
        "@angular/http": "^4.0.0",
        "core-js": "~2.4.1",
        "zone.js": "^0.8.5",
        "rxjs": "^5.2.0",
        "@anglr/common": "^3.0.0"
    },
    "peerDependencies":
    {
        "jquery-param": "^0.2.0",
        "crypto-js": "^3.1.9-1"
    },
    "devDependencies":
    {
        "@types/crypto-js": "^3.1.33",
        "@types/node": "^7.0.8",
        "@anglr/types": "^1.1.2",
        "@angular/compiler-cli": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "rimraf": "~2.6.1",
        "typescript": "2.2.1",
        "webpack": "2.3.2",
        "npm-publish-package": "~1.2.0"
    }
}
{
    "name": "@anglr/rest",
    "version": "3.0.0-beta.0",
    "description": "Angular module representing rest services",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts":
    {
        "build": "npm run ngc",
        "clean": "rimraf dist",
        "clean:deps": "rimraf node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "prepublish": "npm run bundle:package",
        "postpublish": "npm run clean",
        "republish": "npm publish --force",
        "bundle": "webpack",
        "bundle:min": "webpack --env.minify",
        "bundle:package": "npm run clean && npm run build && npm run bundle && npm run bundle:min",
        "deploy:stable": "npp -b",
        "deploy:next": "npp -p -b -s \"beta\" -t \"next\""
    },
    "author": "kukjevov",
    "license": "MIT",
    "dependencies":
    {
        "@angular/core": ">=4.0.0-beta.8",
        "@angular/common": ">=4.0.0-beta.8",
        "@angular/http": ">=4.0.0-beta.8",
        "@angular/platform-browser": ">=4.0.0-beta.8",
        "core-js": "~2.4.1",
        "zone.js": "~0.7.7",
        "rxjs": "^5.1.1",
        "@anglr/common": ">=3.0.0-beta.1"
    },
    "peerDependencies":
    {
        "jquery": "~2.2.1"
    },
    "devDependencies":
    {
        "@types/jquery": "~2.0.40",
        "@angular/compiler-cli": ">=4.0.0-beta.8",
        "@angular/compiler": ">=4.0.0-beta.8",
        "rimraf": "~2.5.4",
        "typescript": "2.1.6",
        "webpack": "2.2.1",
        "npm-publish-package": "~1.2.0"
    }
}
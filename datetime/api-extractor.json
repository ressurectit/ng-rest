{
    "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
    "mainEntryPointFilePath": "./src/index.d.ts",
    "bundledPackages": [],
    "compiler":
    {},
    "apiReport":
    {
        "enabled": true,
        "reportFolder": "./temp/",
        "reportTempFolder": "./temp/"
    },
    "docModel":
    {
        "enabled": true
    },
    "dtsRollup":
    {
        "enabled": true,
        "omitTrimmingComments": true,
        "untrimmedFilePath": "./<unscopedPackageName>.internal.d.ts",
        "betaTrimmedFilePath": "./<unscopedPackageName>.d.ts"
    },
    "tsdocMetadata":
    {},
    "messages":
    {
        "compilerMessageReporting":
        {
            "TS2307": 
            {
                "logLevel": "none"
            },
            "TS7016": 
            {
                "logLevel": "none"
            },
            "default":
            {
                "logLevel": "warning"
            }
        },
        "extractorMessageReporting":
        {
            "ae-missing-release-tag":
            {
                "logLevel": "none"
            },
            "ae-internal-missing-underscore":
            {
                "logLevel": "none"
            },
            "ae-missing-getter":
            {
                "logLevel": "none"
            },
            "ae-forgotten-export":
            {
                "logLevel": "none"
            },
            "default":
            {
                "logLevel": "warning"
            }
        },
        "tsdocMessageReporting":
        {
            "tsdoc-inline-tag-missing-braces":
            {
                "logLevel": "none"
            },
            "default":
            {
                "logLevel": "warning"
            }
        }
    }
}
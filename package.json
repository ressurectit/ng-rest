{
    "name": "@ng2/rest",
    "version": "1.3.2",
    "description": "Angular 2 module representing rest services",
    "main": "index.js",
    "typings": "index.d.ts",
    "scripts":
    {
        "prebuild": "typings install && gulp copy-tstypings",
        "build": "npm run tsc",
        "clean": "rimraf ./*.d.ts ./index.js ./index.dev.js ./index.min.js ./*.map ./*.log ./rest",
        "clean:deps": "rimraf ./node_modules ./typings",
        "clean:all": "npm run clean && npm run clean:deps",
        "tsc": "tsc",
        "tsc:debug": "tsc --sourceMap",
        "tsc:w": "tsc -w --sourceMap",
        "prepublish": "npm run bundle:package",
        "postpublish": "npm run clean",
        "republish": "npm unpublish --force && npm publish",
        "bundle": "tsc --outFile ./index.dev.js",
        "bundle:update": "nab ./index.dev.js && uglifyjs -o ./index.min.js -- ./index.dev.js",
        "bundle:package": "npm run clean && npm run build && npm run bundle && npm run bundle:update",
        "deploy-stable": "npp -b",
        "deploy-next": "npp -b -p -s \"beta\" -t \"next\""
    },
    "author": "kukjevov",
    "license": "MIT",
    "dependencies":
    {
        "@angular/common": "2.0.0-rc.5",
        "@angular/compiler": "2.0.0-rc.5",
        "@angular/core": "2.0.0-rc.5",
        "@angular/http": "2.0.0-rc.5",
        "@angular/platform-browser": "2.0.0-rc.5",
        "core-js": "~2.4.0",
        "reflect-metadata": "0.1.3",
        "zone.js": "0.6.12",
        "rxjs": "5.0.0-beta.6",
        "jquery": "~2.2.1"
    },
    "devDependencies":
    {
        "rimraf": "~2.5.4",
        "typescript": "1.8.10",
        "typings":"~1.3.2",
        "uglify-js": "~2.6.2",
        "gulp": "~3.9.1",
        "through2": "~2.0.1",
        "ng2-tstypings": "~1.1.0",
        "npm-angular2-bundle": "~1.0.1",
        "npm-publish-package": "~1.2.0"
    }
}
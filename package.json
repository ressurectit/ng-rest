{
    "name": "@anglr/rest",
    "version": "0.0.0-placeholder",
    "description": "Angular module representing rest services",
    "module": "./dist/es5/rest.js",
    "esm2015": "./dist/es2015/rest.js",
    "typings": "./rest.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "npm run build:rest && npm run build:stompjs",
        "build:rest": "ngc -p tsconfig.es5.json && ngc -p tsconfig.es2015.json",
        "build:stompjs": "ngc -p stompjs/tsconfig.es5.json && ngc -p stompjs/tsconfig.es2015.json",
        "clean": "rimraf dist tests docs src/**/*.d.ts stompjs/**/*.d.ts */*.d.ts */*.metadata.json ./*.metadata.json ./*.d.ts */dist",
        "clean:duplicates": "rimraf */dist/*/src",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "version:placeholder": "replace-in-file /\\\"version\\\":\\s\\\".*?\\\"/g \"\\\"version\\\": \\\"0.0.0-placeholder\\\"\" package.json --isRegex",
        "version:git": "ngv -e \"replace-in-file 0.0.0-placeholder $GIT_VERSION package.json\"",
        "prepublishOnly": "npm run bundle && npm run version:placeholder && npm run version:git",
        "postpublish": "npm run version:placeholder && npm run clean",
        "bundle": "npm run clean && npm run build && npm run clean:duplicates",
        "deploy": "npm publish",
        "deploy:next": "npm publish --tag next",
        "pretest": "webpack",
        "test": "karma start karma.conf.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ressurectit/ng-rest.git"
    },
    "author": "kukjevov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ressurectit/ng-rest/issues"
    },
    "homepage": "https://github.com/ressurectit/ng-rest",
    "peerDependencies": {
        "@angular/core": ">=8.0.0",
        "@angular/common": ">=8.0.0",
        "rxjs": ">=6.5.0",
        "@jscrpt/common": ">=1.0.2",
        "@anglr/common": ">=6.4.0",
        "jquery-param": "^1.0.1",
        "crypto-js": "^3.1.9-1",
        "@stomp/stompjs": "^5.4.2",
        "sockjs-client": "^1.3.0"
    },
    "devDependencies": {
        "@angular/core": "^8.0.0",
        "@angular/common": "^8.0.0",
        "rxjs": "^6.5.0",
        "@jscrpt/common": "^1.0.2",
        "@anglr/common": "^6.4.0",
        "@anglr/types": "1.1.15",
        "@stomp/stompjs": "^5.4.2",
        "@types/jasmine": "~2.8.7",
        "@types/crypto-js": "^3.1.43",
        "@types/sockjs-client": "^1.1.1",
        "@angular/compiler-cli": "^8.0.0",
        "@angular/compiler": "^8.0.0",
        "@angular/language-service": "^8.0.0",
        "@angular/platform-browser-dynamic": "^8.0.0",
        "@angular/platform-browser": "^8.0.0",
        "tsickle": "0.35.0",
        "karma-cli": "1.0.1",
        "karma": "3.1.1",
        "karma-jasmine": "1.1.2",
        "karma-phantomjs-launcher": "1.0.4",
        "karma-chrome-launcher": "2.2.0",
        "karma-webpack": "3.0.5",
        "karma-jasmine-html-reporter": "1.4.0",
        "jasmine": "3.3.0",
        "awesome-typescript-loader": "5.2.1",
        "angular2-template-loader": "0.6.2",
        "raw-loader": "1.0.0",
        "sass-loader": "7.1.0",
        "css-loader": "1.0.1",
        "file-loader": "3.0.1",
        "style-loader": "0.23.1",
        "@compodoc/compodoc": "1.1.9",
        "rimraf": "2.6.3",
        "copyfiles": "2.1.0",
        "replace-in-file": "4.1.0",
        "typescript": "3.4.5",
        "webpack": "4.32.2",
        "npm-git-version": "1.1.1"
    }
}

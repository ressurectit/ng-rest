{
    "name": "ng2-rest",
    "version": "1.0.8",
    "description": "Angular 2 module representing rest services",
    "scripts":
    {
        "prebuild": "typings install",
        "build": "npm run tsc",
        "clean": "rimraf ./*.d.ts ./common.js ./*.map ./*.log ./bundle",
        "clean:deps": "rimraf ./node_modules ./typings",
        "clean:all": "npm run clean && npm run clean:deps",
        "tsc": "tsc",
        "tsc:debug": "tsc --sourceMap",
        "tsc:w": "tsc -w --sourceMap",
        "prepublish": "npm run bundle:package",
        "postpublish": "npm run clean",
        "bundle": "tsc --outFile bundle/ng2-rest.dev.js",
        "bundle:update": "nab bundle/ng2-rest.dev.js && uglifyjs -o bundle/ng2-rest.min.js -- bundle/ng2-rest.dev.js",
        "bundle:package": "npm run clean && npm run build && npm run bundle && npm run bundle:update",
        "deploy-stable": "npp -b",
        "deploy-next": "npp -b -p -s \"beta\" -t \"next\""
    },
    "author": "kukjevov",
    "license": "MIT",
    "dependencies":
    {
        "angular2": "2.0.0-beta.8",
        "es6-promise": "^3.0.2",
        "es6-shim": "^0.33.3",
        "reflect-metadata": "0.1.2",
        "zone.js": "0.5.15",
        "rxjs": "5.0.0-beta.2",
        "jquery": "~2.2.1"
    },
    "devDependencies":
    {
        "rimraf": "~2.5.1",
        "typescript": "~1.8.9",
        "typings":"^0.7.9",
        "uglify-js": "~2.6.2",
        "npm-angular2-bundle": "1.0.0",
        "npm-publish-package": "1.2.0"
    }
}